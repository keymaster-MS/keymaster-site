#   爪转 转 转拽转
Options -Indexes

#  住转 砖 住 拽爪 专砖
<FilesMatch "\.(ini|env|log|md|txt|yml|yaml|json|config)$">
  Require all denied
</FilesMatch>

#  住转 转拽 住驻爪驻转 (砖 assets/private)
RewriteEngine On
RewriteRule ^assets/private/ - [F,L]

#  注转 Hotlinking 转转 (驻砖专 住祝 住转 住驻转)
RewriteCond %{HTTP_REFERER} !^https?://(www\.)?keymaster-ms\.com/ [NC]
RewriteCond %{HTTP_REFERER} !^$ 
RewriteRule \.(jpg|jpeg|png|gif|webp|svg)$ - [F,NC,L]

#  驻转 HTTPS
RewriteCond %{HTTPS} !=on
RewriteRule ^ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]

#  住转 拽爪 
<Files "web.html">
  Require all denied
</Files>
